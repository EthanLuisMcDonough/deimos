program {
    let count: i32 = 0, index: i32 = 0;

    print "Enter the number of numbers you'd like to fizzbuzz: ";
    call read_i32(&count);

    while (index < count) {
        if (index % 3 == 0 and index % 5 == 0) {
            print "Fizzbuzz\n";
        } elif (index % 3 == 0) {
            print "Fizz\n";
        } elif (index % 5 == 0) {
            print "Buzz\n";
        } else {
            print index, "\n";
        }
        index = index + 1;
    }
}
